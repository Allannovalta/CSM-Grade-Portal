@using CSM.GradePortal.Web.Infrastructure.Data.Models;
@model CSM.GradePortal.Web.ViewModels.Users.IndexViewModel;
@{
    ViewData["Title"] = "WELCOME TO CSM GRADE PORTAL";
}

<h2>Users</h2>
<ul>
    @{foreach (User user in Model.Users)
        {
            <li>@user.FullName    @user.Gender.ToString()  @user.EmailAddress</li>
        }
    }
</ul>
